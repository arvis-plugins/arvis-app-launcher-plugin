{
    "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/plugin-schema.json",
    "category": "Productivity",
    "creator": "jopemachine",
    "description": "App launcher plugin for Arvis",
    "defaultIcon": "icon.png",
    "name": "arvis-app-launcher-plugin",
    "readme": "https://github.com/jopemachine/arvis-app-launcher-plugin",
    "version": "0.0.1",
    "webAddress": "https://github.com/jopemachine/arvis-app-launcher-plugin",
    "enabled": true,
    "main": "index.js",
    "actions": [
        {
            "modifiers": "normal",
            "type": "script",
            "script": "node open.js '{query}'"
        }
    ]
}
